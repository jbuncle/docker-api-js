        "lint-fix": "eslint --fix -c .eslintrc.json --ext .ts ./src"
    },
    "dependencies": {
        "@jbuncle/core-js": "^1.2.0",
        "@jbuncle/logging-js": "^1.92.0",
        "node-forge": "^0.10.0"
    },
    "devDependencies": {
        "@jbuncle/eslint-config-jbuncle": "^1.6.0",
        "@types/node": "^14.14.27",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.5"
    }
}
