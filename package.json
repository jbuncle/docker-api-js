{
    "name": "@jbuncle/docker-api-js",
    "version": "1.0.0",
    "description": "Template generation from docker",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "LGPL-3.0-only",
    "scripts": {
        "compile": "tsc --strict -p ./tsconfig.json",
        "build": "npm run lint && npm run compile",
        "lint": "eslint -c .eslintrc.json --ext .ts ./src",
        "lint-fix": "eslint --fix -c .eslintrc.json --ext .ts ./src"
    },
    "dependencies": {
        "@jbuncle/core-js": "^1.3.5",
        "@jbuncle/logging-js": "^1.92.0",
        "node-forge": "^1.3.1"
    },
    "devDependencies": {
        "@jbuncle/eslint-config-jbuncle": "github:jbuncle/eslint-config-jbuncle",
        "@types/node": "^18.11.16",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
