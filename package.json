    },
    "dependencies": {
        "@jbuncle/core-js": "^1.3.6",
        "@jbuncle/logging-js": "^2.0.1",
        "node-forge": "^1.3.1"
    },
    "devDependencies": {
        "@jbuncle/eslint-config-jbuncle": "^1.6.15",
        "@types/node": "^18.11.17",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
